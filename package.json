{
  "name": "tumblrinbrowser",
  "version": "0.0.0",
  "author": "kthjm <toxictoxer@gmail.com>",
  "repository": "kthjm/tumblrinbrowser",
  "license": "MIT",
  "main": "v2/cjs.js",
  "module": "v2/es.js",
  "jsnext:main": "v2/es.js",
  "nyc": {
    "include": ["src"],
    "require": [
      "babel-register",
      "dotenv/config"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "test": "cross-env NODE_ENV=TEST nyc mocha --recursive -t 10000",
    "prebuild": "yarn test",
    "build": "cross-env NODE_ENV=BUILD rollup -c",
    "env": "cross-env NODE_ENV=ENV node -r babel-register -r dotenv/config",
    "snap": "yarn env responses/index.js"
  },
  "dependencies": {
    "jsonp-simple": "^0.0.5",
    "tiloop": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-power-assert": "^2.0.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "dotenv": "^5.0.1",
    "fs-extra": "^5.0.0",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.2",
    "node-fetch": "^2.1.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4",
    "rewire": "^3.0.2",
    "rollup": "^0.56.4",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-prettier": "^0.4.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sinon": "^4.4.2",
    "uglify-es": "^3.3.9"
  }
}
